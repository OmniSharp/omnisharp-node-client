    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.4.2",
    "strip-bom": "^3.0.0",
    "ts-disposables": "^2.2.3",
    "typings": "^2.1.1",
    "vscode-jsonrpc": "^3.3.1",
    "vscode-languageserver": "^3.3.0"
  },
  "devDependencies": {
